        <mxCell id="be_sql1" parent="swim_be" style="text;html=1;whiteSpace=wrap;strokeColor=#d6b656;fillColor=#fff2cc;align=left;verticalAlign=middle;rounded=0;" value="SELECT von.notice_no, von.vehicle_no, von.offence_notice_type,&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;von.last_processing_stage, von.suspension_type,&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;sn.reason_of_suspension, von.amount_paid&lt;/div&gt;&lt;div&gt;FROM ocms_valid_offence_notice von&lt;/div&gt;&lt;div&gt;LEFT JOIN ocms_suspended_notice sn&lt;/div&gt;&lt;div&gt;&amp;nbsp; ON von.notice_no = sn.notice_no&lt;/div&gt;&lt;div&gt;&amp;nbsp; AND sn.sr_no = (SELECT MAX(sr_no) FROM ocms_suspended_notice WHERE notice_no = von.notice_no)&lt;/div&gt;&lt;div&gt;WHERE von.notice_no LIKE &#39;%&#39; || :noticeNo || &#39;%&#39;&lt;/div&gt;&lt;div&gt;&amp;nbsp; AND von.last_processing_stage IN (:stages)&lt;/div&gt;&lt;div&gt;ORDER BY von.notice_no&lt;/div&gt;&lt;div&gt;LIMIT :limit OFFSET :skip&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;Note: reason_of_suspension from child table (latest suspension)&lt;/div&gt;" vertex="1">
